<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Latitude</th>
    <th>Longitude</th>
  </tr>
  <% for user in @users %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.address %></td>
      <td><%= user.latitude %></td>
      <td><%= user.longitude %></td>
    </tr>
  <% end %>
</table>
